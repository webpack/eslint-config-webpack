{
  "name": "eslint-config-webpack",
  "version": "4.9.0",
  "description": "Provides Webpack's eslint rules as an extensible shared config",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "webpack",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/webpack/eslint-config-webpack#readme",
  "bugs": {
    "url": "https://github.com/webpack/eslint-config-webpack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack/eslint-config-webpack.git"
  },
  "license": "MIT",
  "author": "Joshua Wiens (https://twitter.com/@d3viant0ne)",
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js",
    "plugins",
    "prettier-config.js",
    "prettier-config-es5.js",
    "configs.js",
    "ignore-paths.js",
    "configs"
  ],
  "scripts": {
    "lint": "npm run lint:code",
    "lint:code": "eslint --cache .",
    "release": "standard-version"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/markdown": "^7.5.1",
    "@stylistic/eslint-plugin": "^5.7.0",
    "detect-indent": "^7.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jsdoc": "^62.4.1",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "semver": "^7.7.3",
    "sort-package-json": "^3.6.0",
    "typescript-eslint": "^8.53.1"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-find-rules": "^5.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.3"
